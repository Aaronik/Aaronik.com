<!-- Iframe wrapper for anything else it's needed for -->
<!-- resume iframe -->
<div id="blank_iframe_wrapper" class="program_iframe_wrapper">
	<div class="iframe_button_wrappers">
		<div class="close_button">Close</div>
		<div id="resume_download_button">Download Resume</div>
		<iframe id="resume_iframe" class="program_iframes" src="aaron_sullivan_resume.html"></iframe>
	</div>
</div>

<% programs.each do |prog| %>
	<div id="<%= prog %>_iframe_wrapper" class="program_iframe_wrapper">
		<div class="iframe_button_wrappers"><div class="close_button">Close</div><div class="github_button" id="<%= prog %>">View in Github</div></div>
		<iframe id="<%= prog %>_iframe" class="program_iframes" srcdoc="Loading...">your browser no iframe</iframe>
	</div>
<% end %>